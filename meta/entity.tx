EntityModel:
    simpleTypes *= SimpleType
    entities += Entity
    packages += Package
    (configs = Config)?
;

Config:
    'config' '{'
        config_params *= ConfigParam[';']
    '}'
;

ConfigParam:
    key=ID ':' value=STRING
;

Package:
    'package' name=ID '{'
        childPackage*=Package
    '}'
;

EntityPackage:
    'package' '->' package = [Package]
;

Entity:
    'entity' name = ID '{'
        (belongsTo = EntityPackage)?
        properties *= Property
    '}'
;

Property:
    name = ID ':' type = [Type]
;

Type:
    SimpleType | Entity
;

SimpleType:
    'type' name = ID
;
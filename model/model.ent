entity User {
    package -> child
    id: long
    firstName: string
    lastName: string
    username: string
    password: string
    address: Address
    active: boolean
}

entity Address {
    id: long
    street: string
    number: int
    zipCode: int
}

package base {
    package child {

    }
}

config {
        dependencyInjectionType : 'constructor';
        idType : 'Integer';
        applicationName: 'MyApplication'
}
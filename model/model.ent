entity User {
    package -> child
    id: long
    firstName: string
    lastName: string
    username: string
    password: string
    address: Address
    active: boolean
}

entity Address {
    id: long
    street: string
    number: int
    zipCode: int
}

package base {
    package child {

    }
}

config {
        dependencyInjectionType : 'setter';
        idType : 'Integer';
        applicationName: 'MyApplication';
        serverPort: '8081';
        contextPath: '/myapp';
        dataSource: 'h2';
        dataSourceUrl: 'jdbc:h2:mem:db;DB_CLOSE_DELAY=-1;DB_CLOSE_ON_EXIT=FALSE';
        dataSourceUsername: 'sa';
        dataSourcePassword: 'sa';
        hibernateDialect: 'org.hibernate.dialect.H2Dialect';
        driverClassName: 'org.h2.Driver'
}
